{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 0,
        "no-param-reassign": ["error", {"props": false}]
    },
    "ignorePatterns": [
        "node_modules/*",
        "cypress/*",
        "*.config.*"
    ]
}